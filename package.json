{
  "name": "roessing.de",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev --host 127.0.0.1",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mdx,.astro,.cjs,.mjs --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "unlighthouse": "unlighthouse-ci --site http://127.0.0.1:8080",
    "performance:check": "start-server-and-test 'bun http-server ./dist' 8080 'bun run unlighthouse'",
    "performance:build": "start-server-and-test 'bun http-server ./dist' 8080 'bun run unlighthouse --build-static'",
    "postinstall": "bun x @puppeteer/browsers install chrome@stable --path $HOME/.cache/puppeteer",
    "postinstall:comment": "https://github.com/oven-sh/bun/issues/4705#issuecomment-1712545662",
    "cypress:open": "cypress open",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/mdx": "^1.1.5",
    "@astrojs/react": "^3.0.5",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "astro": "^3.5.5",
    "astro-navbar": "^2.3.0",
    "classnames": "^2.3.2",
    "fp-ts": "^2.16.1",
    "fp-ts-std": "^0.18.0",
    "io-ts": "^2.2.20",
    "ramda": "^0.29.1",
    "sharp": "0.33.0-alpha.11",
    "tailwindcss": "^3.0.24",
    "typescript": "^5.2.2",
    "usehooks-ts": "^2.9.1",
    "vitest": "^0.34.6"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.3.1",
    "@types/ramda": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@unlighthouse/cli": "^0.10.2",
    "cypress": "^13.5.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-mdx": "^2.2.0",
    "eslint-plugin-storybook": "^0.6.15",
    "http-server": "^14.1.1",
    "lint-staged": "^15.1.0",
    "node-gyp": "^10.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "puppeteer": "^21.5.2",
    "start-server-and-test": "^2.0.3"
  },
  "trustedDependencies": [
    "puppeteer",
    "sharp",
    "cypress"
  ]
}
